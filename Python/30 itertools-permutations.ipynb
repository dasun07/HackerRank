    "from itertools import permutations\n",
    "\n",
    "string, k = input().split()\n",
    "k = int(k)\n",
    "\n",
    "perm = sorted([''.join(list(i)) for i in list(permutations(string,k))])\n",
    "for i in perm:\n",
    "    print(i)\n",
